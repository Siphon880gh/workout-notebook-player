{
    "name": "workout-notebook",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "babel-preset-es2015": "^6.24.1",
        "dree": "^3.4.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.8.1",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.18.6",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "babel-jest": "^27.5.1",
        "jest": "^27.5.1",
        "jsdom": "^16.7.0"
    },
    "scripts": {
        "start": "node public/data/update-paths; cp public/data/paths.json src/paths.json; react-scripts start",
        "build": "node public/data/update-paths; cp public/data/paths.json src/paths.json; react-scripts build",
        "test-old": "react-scripts test",
        "test": "clear; jest test",
        "eject": "react-scripts eject"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "node_modules"
        ],
        "transform": {
            "^.+\\.(js|jsx)$": "babel-jest"
        },
        "testEnvironment": "jsdom"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
